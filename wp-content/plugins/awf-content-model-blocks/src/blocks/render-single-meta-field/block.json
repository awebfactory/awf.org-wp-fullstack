{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "awebfactory/render-single-meta-field",
	"version": "0.1.0",
	"title": "Render Single Meta Field",
	"category": "text",
	"icon": "schedule",
	"description": "A custom block for rendering the specified field of a specified CPT\"",
	"supports": {
		"html": false
	},
	"attributes": {
		"selectedPostType": {
			"type": "string",
			"default": "post"
		},
		"selectedPostField": {
			"type": "string"
		}
	},
	"textdomain": "render-single-meta-field",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
